{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://pdfmaster-wp-production.up.railway.app/$1",
      "headers": {
        "X-Forwarded-Host": "$host",
        "X-Forwarded-Proto": "https"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
