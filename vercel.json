{
  "version": 2,
  "builds": [],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://pdfmaster-wp-production.up.railway.app/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Forwarded-Host",
          "value": "$host"
        },
        {
          "key": "X-Forwarded-Proto",
          "value": "https"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
